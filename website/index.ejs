        <div style="padding-top: 2rem;width: 100%;" class="search">
          <center>
          <input type="text"/ class="search searchbox" id="searchinput">
          <input type="image" src="./svgs/search-50.svg" class="search searchbutton" onclick="searchUpdate(document.querySelector('#searchinput'))"/>
        </center>
        </div>

        <script>
          function searchUpdate(self) {
            const extensions = Array.from(document.querySelectorAll('div.extension'));
            const hidden = [];
            const no_query = document.querySelector('h3#no_query');
            extensions.forEach(el => {
              const content = el.textContent.toLowerCase();
              const val = self.value.toString().toLowerCase();
              if (content.includes(val)) {
                el.hidden = false;
              } else { el.hidden = true };
              hidden.push(el.hidden);
            });
            if (hidden.every(Boolean)) {
              no_query.hidden = false;
            } else {
              no_query.hidden = true;
            }
          }
        </script>
      </div>

